{
  "framework": "vue",
  "name": "vue-cms-components",
  "version": "1.0.13",
  "description-markup": "markdown",
  "contributions": {
    "html": {
      "vue-components": [
        {
          "name": "VccNotifications",
          "description": "Notifications and message",
          "props": [
            {
              "name": "list",
              "description": "message data",
              "type": "any[]"
            },
            {
              "name": "tabList",
              "description": "tab data",
              "type": "NotificationTab[]"
            },
            {
              "name": "hintText",
              "description": "hint text",
              "type": "string"
            },
            {
              "name": "headerList",
              "description": "渲染header 列表",
              "type": "array"
            },
            {
              "name": "trigger",
              "description": "触发方式",
              "type": "string"
            },
            {
              "name": "moveAnimate",
              "description": "是否启用已读动画",
              "type": "boolean"
            },
            {
              "name": "itemClass",
              "description": "message item自定义class",
              "type": "string"
            },
            {
              "name": "finishText",
              "description": "设置结束描述字",
              "type": "string"
            },
            {
              "name": "messageProps",
              "description": "message的自定义属性配置",
              "type": "string"
            },
            {
              "name": "popperClass",
              "description": "弹出层的自定义class",
              "type": "string"
            }
          ],
          "slots": [
            {
              "name": "default",
              "description": "自定义通知按钮"
            },
            {
              "name": "header",
              "description": "设置自定义通知header区域"
            },
            {
              "name": "message",
              "description": "自定义消息节点",
              "type": "{data: any}"
            }
          ]
        },
        {
          "name": "VccIconButton",
          "description": "icon and button",
          "props": [
            {
              "name": "icon",
              "description": "icon class",
              "type": "string"
            },
            {
              "name": "type",
              "description": "button type",
              "type": "'info' | 'success' | 'danger' | 'warning' | 'primary'"
            },
            {
              "name": "disabled",
              "description": "disabled",
              "type": "boolean"
            },
            {
              "name": "label",
              "description": "label text",
              "type": "string"
            },
            {
              "name": "color",
              "description": "icon color",
              "type": "string"
            }
          ]
        },
        {
          "name": "VccFileUploader",
          "description": "file upload components",
          "props": [
            {
              "name": "currentFiles",
              "description": "文件集合对象",
              "type": "array"
            },
            {
              "name": "action",
              "description": "upload url",
              "type": "string"
            },
            {
              "name": "name",
              "description": "upload上传时文件的key",
              "default": "file",
              "type": "string"
            },
            {
              "name": "requestMethod",
              "description": "upload上传method",
              "default": "post",
              "type": "string"
            },
            {
              "name": "params",
              "description": "upload 参数",
              "type": "object"
            },
            {
              "name": "headers",
              "description": "upload headers 请求头",
              "type": "object"
            },
            {
              "name": "readOnly",
              "description": "是否只读",
              "type": "boolean"
            },
            {
              "name": "showHint",
              "description": "是否显示文件类型提示",
              "type": "boolean"
            },
            {
              "name": "imgOnly",
              "description": "是否只能上传图片",
              "type": "boolean"
            },
            {
              "name": "limit",
              "description": "文件上传数量限制",
              "default": "5",
              "type": "number"
            },
            {
              "name": "fileSize",
              "description": "文件上传大小限制(MB)",
              "default": "30",
              "type": "number"
            }
          ]
        }
      ]
    }
  }
}
